pseudo-device	loop			        			# network loopback
#pseudo-device	mpls							# MPLS pseudo-interface
pseudo-device	ppp		    2					# Point-to-Point Protocol
#pseudo-device	pppoe			  				# PPP over Ethernet (RFC 2516)
pseudo-device	sl		    2					# Serial Line IP
